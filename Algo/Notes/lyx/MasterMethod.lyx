\begin_layout Standard
\begin_inset Formula 
\[
T(n)=\begin{cases}
O(n^{d}\log n) & \text{if }a=b^{d}\\
O(n^{d}) & \text{if }a<b^{d}\\
O(n^{\log_{b}a}) & \text{if }a>b^{d}
\end{cases}
\]

\end_inset


\end_layout

