\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Total\, work\le cn^{d}\times\sum_{j=0}^{\log_{b}n}\left[\frac{a}{b^{d}}\right]^{j}\qquad{}^{(*)}
\]

\end_inset


