\begin_layout Standard

\series bold
If 
\begin_inset Formula $a=b^{d}$
\end_inset


\series default
, then 
\begin_inset Formula 
\begin{eqnarray*}
total\, work & = & cn^{d}(\log_{b}n+1)\\
 & = & O(n^{d}\log n)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard

\series bold
If 
\begin_inset Formula $a<b^{d}$
\end_inset


\series default
, then
\end_layout

\begin_layout Standard
call 
\begin_inset Formula $\frac{a}{b^{d}}:=r\le$
\end_inset

a constant (independent of n)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
total\, work=O(n^{d})
\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
If 
\begin_inset Formula $a>b^{d}$
\end_inset


\series default
, then
\end_layout

\begin_layout Standard
call 
\begin_inset Formula $\frac{a}{b^{d}}:=r>1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
total\, work=O(n^{d}\times(\frac{a}{b^{d}})^{\log_{b}n})
\]

\end_inset


\end_layout

\begin_layout Standard
Note: 
\begin_inset Formula $b^{-d\log_{b}n}=(b^{\log_{b}n})^{-d}=n^{-d}$
\end_inset

, so
\end_layout

\begin_layout Standard
